package pack03_OOP;

import java.util.Arrays;

public class StudentMain {
	public static void main(String[] args) {
		String[] subject = {"과목1","과목2","과목3"} ;
		StudentDTO dtos[] =new StudentDTO[3];
		dtos[0] = new StudentDTO("홍길동", subject );
		dtos[1] = new StudentDTO("홍길동", subject );
		dtos[2] = new StudentDTO("홍길동", subject );
			//	
		for (int i = 0; i < dtos.length; i++) {
			System.out.println(dtos[i].name);
			System.out.println(Arrays.toString( dtos[i].subject ));
			System.out.println(Arrays.toString( dtos[i].scores ));
		}
	
	}
}
