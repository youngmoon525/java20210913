package pack03_OOP;

import java.util.Random;
import java.util.Scanner;

public class StudentMain {
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		String[] subjects = {"java" , "css" , "javaScript"};
		Random r = new Random();
		int[] scores = new int[subjects.length];
		for (int i = 0; i < scores.length; i++) {
			scores[i] = r.nextInt(100)+1;
		}
		StudentDTO dto = new StudentDTO("TEST", subjects, scores);
		for (int i = 0; i < subjects.length; i++) {
			System.out.print(dto.name );// index int값, score int값
			System.out.println(dto.subjects[i] + " 점수 : " + dto.scores[i]);
		}
	}
}
