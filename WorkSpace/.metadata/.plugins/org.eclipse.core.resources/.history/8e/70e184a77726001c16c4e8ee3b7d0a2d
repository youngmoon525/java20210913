package pack03_Constructor;

public class Ex02_Car {
	String company ;
	String model;
	String color;
	int maxSpeed;
	int speed;
	
	public Ex02_Car(String company, String model) {
		super();
		this.company = company;
		this.model = model;
	}

	public Ex02_Car(String company, String model, String color, int maxSpeed, int speed) {
		super();
		this.company = company;
		this.model = model;
		this.color = color;
		this.maxSpeed = maxSpeed;
		this.speed = speed;
	}
	
}
