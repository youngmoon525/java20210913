	//필드
	byte byteField;
	short shortField;
	int intField;
	long longField;
	
	boolean booleanField;
	char charField;
	
	float floatField;
	double doubleField;
	//대부분의 자바 변수 모든것(Button ,EditText 어떠한 컨트롤이건) 자바코드로 된것들은
	//필드로 사용할수있다.
	int[] arr1Field;
	String[] arr2Field;
	
	
	
	
	

}
